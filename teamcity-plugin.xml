<?xml version="1.0" encoding="UTF-8"?>
<teamcity-plugin xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:schemas-jetbrains-com:teamcity-plugin-v1-xml">
  <info>
    <name>manta-artifact-storage</name>
    <display-name>Triton Manta Artifact Storage</display-name>
    <version>@Version@</version>
    <description>Allows storing build artifacts in Triton Manta</description>
    <download-url>https://github.com/joyent/teamcity-manta-artifact-storage-plugin</download-url>
    <vendor>
      <name>@VendorName@</name>
      <url>@VendorURL@</url>
    </vendor>
  </info>
  <deployment use-separate-classloader="true" />
</teamcity-plugin>
